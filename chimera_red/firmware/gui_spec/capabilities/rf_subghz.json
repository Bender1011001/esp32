{
    "module": "CC1101",
    "bus": "SPI",
    "frequency_range_mhz": [
        300,
        928
    ],
    "default_freq": 433.92,
    "functions": [
        {
            "name": "init",
            "blocking": true,
            "duration_ms": 100
        },
        {
            "name": "setFreq",
            "params": [
                "float freq_mhz"
            ],
            "blocking": true,
            "duration_ms": 10
        },
        {
            "name": "setRx",
            "blocking": false,
            "note": "Enters RX state, awaits GDO0 interrupt"
        },
        {
            "name": "sendData",
            "params": [
                "byte* data",
                "int len"
            ],
            "blocking": true,
            "duration_ms": "len * byte_time"
        },
        {
            "name": "getRssi",
            "return": "int16_t",
            "blocking": true,
            "duration_ms": 1
        }
    ]
}